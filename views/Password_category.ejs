<%-include('header')%>
            
<div class="container-fluid">
        
    <div class="container">
            <h1 style="margin: 50px auto; text-align: left; font-size: 24px; text-decoration: underline orange;"><%= title %></h1>
    </div>

    <div class="container">
      <table class="table table-striped table-bordered table-hover" style="text-align: center;">
        <thead">
          <tr>
            <th style="text-align: left;">Password Category</th>
            <th style="text-align: center;">Action</th>
          </tr>
        </thead>
        <tbody>
          <% if(records.length >0)
          {records.forEach(function(row){%>
            <tr>
                <td style="text-align: left;"><%= row.password_category%></td>
                <td><a class="btn btn-primary btn-xs" href="/passwordCategory/edit/<%= row._id%>"><i class="fa fa-edit"></i></a>
                <a class="btn btn-danger btn-xs" href="/passwordCategory/delete/<%= row._id%>"><i class="fa fa-trash"></i></a></td>
            </tr>
              <%}) }%>
        </tbody>
      </table>
    </div>

   

 </div> 
<%-include('footer')%>
